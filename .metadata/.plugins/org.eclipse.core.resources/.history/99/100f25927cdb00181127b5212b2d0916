package application;

import java.util.Locale;
import java.util.Scanner;

public class Program {

	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		int x = sc.nextInt();
		
		for(1; x;x++) {
			if (x%2 !=0) {
				System.out.println(x);
			}
		}
		
		
		
		sc.close();

	}

}
