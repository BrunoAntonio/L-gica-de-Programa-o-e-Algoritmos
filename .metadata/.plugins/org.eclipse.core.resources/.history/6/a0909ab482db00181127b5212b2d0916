package application;

import java.util.Locale;
import java.util.Scanner;

public class Program {

	public static void main(String[] args) {

		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);

		int n = sc.nextInt();

		int fact=1;
		for (int i = 0; i < n; i++) {
			fact *=(n-i);

		}
		System.out.println(fact);

		sc.close();

	}

}
